<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<VBox xmlns:fx="http://javafx.com/fxml" stylesheets="/css/editorSheet.css" styleClass="dialogControlButtons">
    <VBox fx:id="content"><TextField fx:id="stringTextField"></TextField></VBox>

    <GridPane styleClass="buttons" hgap="10">
        <Button GridPane.columnIndex="0" GridPane.rowIndex="0" styleClass="okayButton" fx:id="AcceptButton">OK</Button>
        <Button GridPane.columnIndex="1" GridPane.rowIndex="0" styleClass="okayButton" fx:id="CancelButton">CANCEL</Button>
    </GridPane>
</VBox>